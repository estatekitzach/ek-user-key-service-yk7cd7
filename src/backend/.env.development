# Application Environment
ASPNETCORE_ENVIRONMENT=Development

# AWS Configuration
AWS_REGION=us-east-1
AWS_KMS_KEY_ID=alias/estatekit-dev-key
AWS_USE_IAM_ROLE=true
AWS_COGNITO_USER_POOL_ID=us-east-1_devpool
AWS_COGNITO_APP_CLIENT_ID=dev-client-id
AWS_USE_LOCALSTACK=true
AWS_REQUEST_TIMEOUT=30

# Database Configuration
DB_CONNECTION_STRING=Host=localhost;Port=5432;Database=estatekit;Username=dev_user;Password=dev_password;Include Error Detail=true
DB_SCHEMA=estatekit
DB_COMMAND_TIMEOUT=30
DB_ENABLE_RETRY=true
DB_MAX_RETRY_COUNT=3
DB_MAX_RETRY_DELAY=30
DB_ENABLE_SENSITIVE_LOGGING=true
DB_ENABLE_DETAILED_ERRORS=true

# Redis Cache Configuration
REDIS_CONNECTION_STRING=localhost:6379
REDIS_INSTANCE_NAME=estatekit-dev
REDIS_DATABASE_ID=0
REDIS_TTL_MINUTES=15
REDIS_ENABLE_COMPRESSION=true
REDIS_RETRY_COUNT=3
REDIS_RETRY_DELAY_MS=100

# Key Rotation Settings
KEY_ROTATION_INTERVAL_DAYS=90
KEY_ROTATION_COMPLIANCE_DAYS=365
KEY_ROTATION_EMERGENCY_TIMEOUT=300
KEY_ROTATION_MAX_RETRIES=3
KEY_ROTATION_RETRY_DELAY=30
KEY_ROTATION_ENABLE_AUTOMATIC=true
KEY_ROTATION_LOCK_TIMEOUT=900
KEY_ROTATION_MIN_AGE_DAYS=1
KEY_ROTATION_REQUIRE_AUDIT=true

# Development & Debugging
LOGGING_LEVEL=Debug
ENABLE_SWAGGER=true
ENABLE_DETAILED_ERRORS=true
ENABLE_SENSITIVE_DATA_LOGGING=true
ENABLE_DEVELOPMENT_EXCEPTIONS=true