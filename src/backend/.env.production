ASPNETCORE_ENVIRONMENT=Production

# AWS Configuration
AWS_REGION=us-east-1
AWS_KMS_KEY_ID=${aws_kms_key_id}
AWS_USE_IAM_ROLE=true
AWS_COGNITO_USER_POOL_ID=${aws_cognito_user_pool_id}
AWS_COGNITO_APP_CLIENT_ID=${aws_cognito_app_client_id}
AWS_REQUEST_TIMEOUT=30

# Database Configuration
DATABASE_CONNECTION_STRING=Server=${rds_endpoint};Port=5432;Database=estatekit;User Id=${db_username};Password=${db_password};SSL Mode=Require;Trust Server Certificate=true
DATABASE_SCHEMA=estatekit
DATABASE_COMMAND_TIMEOUT=30
DATABASE_ENABLE_RETRY=true
DATABASE_MAX_RETRY_COUNT=3
DATABASE_MAX_RETRY_DELAY=30

# Redis Cache Configuration
REDIS_CONNECTION_STRING=${redis_connection_string}
REDIS_INSTANCE_NAME=estatekit-prod
REDIS_DATABASE_ID=0
REDIS_TTL_MINUTES=15
REDIS_ENABLE_COMPRESSION=true
REDIS_RETRY_COUNT=3
REDIS_RETRY_DELAY_MS=100

# Key Rotation Settings
KEY_ROTATION_INTERVAL_DAYS=90
KEY_ROTATION_COMPLIANCE_INTERVAL_DAYS=365
KEY_ROTATION_EMERGENCY_TIMEOUT_MINUTES=5
KEY_ROTATION_MAX_RETRY_ATTEMPTS=3
KEY_ROTATION_RETRY_DELAY_SECONDS=30
KEY_ROTATION_ENABLE_AUTOMATIC=true
KEY_ROTATION_LOCK_TIMEOUT_MINUTES=15
KEY_ROTATION_MINIMUM_AGE_DAYS=1
KEY_ROTATION_REQUIRE_AUDIT_LOGGING=true

# Logging and Debug
LOGGING_LEVEL=Information
ENABLE_SWAGGER=false